{
  "name": "react-chrome-extension",
  "version": "0.0.1",
  "description": "Build an Extension!",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html"
  },
  "options_page": "index.html",
  "icons": {
    "192": "logo192.png",
    "512": "logo512.png"
  },
  "manifest_version": 2
}